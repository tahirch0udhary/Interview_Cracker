{
  "name": "interviewcracker-electron",
  "version": "0.2.0",
  "private": true,
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev:react": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env NODE_ENV=development node node_modules/electron/cli.js .",
    "dev": "concurrently --kill-others \"npm:dev:react\" \"npm:dev:electron\"",
    "build:react": "vite build",
    "start": "node node_modules/electron/cli.js .",
    "package": "electron-builder"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@tailwindcss/typography": "^0.5.19",
    "adm-zip": "^0.5.16",
    "cross-fetch": "^3.1.5",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "openai": "^6.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.0",
    "cross-env": "^10.1.0",
    "electron": "^33.0.0",
    "electron-builder": "^24.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.1"
  }
}
